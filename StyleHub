local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
screenGui.ResetOnSpawn = false

-- Текст "By" в центре
local byLabel = Instance.new("TextLabel")
byLabel.Size = UDim2.new(0, 200, 0, 50)
byLabel.Position = UDim2.new(0.5, -100, 0.5, -25)
byLabel.Text = "By"
byLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
byLabel.TextSize = 40
byLabel.Font = Enum.Font.Gotham
byLabel.BackgroundTransparency = 1
byLabel.Parent = screenGui

-- Текст для "nig2aotsos"
local mainLabel = Instance.new("TextLabel")
mainLabel.Size = UDim2.new(0, 200, 0, 50)
mainLabel.Position = UDim2.new(0.5, -100, 0.5, 25)
mainLabel.Text = ""
mainLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
mainLabel.TextSize = 30
mainLabel.Font = Enum.Font.Gotham
mainLabel.BackgroundTransparency = 1
mainLabel.Parent = screenGui

-- Loading bar
local loadingBarFrame = Instance.new("Frame")
loadingBarFrame.Size = UDim2.new(0, 200, 0, 10)
loadingBarFrame.Position = UDim2.new(0.5, -100, 0.5, 70) -- Below nig2aotsos
loadingBarFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
loadingBarFrame.BackgroundTransparency = 0.5
loadingBarFrame.BorderSizePixel = 0
loadingBarFrame.Parent = screenGui

local loadingBarFill = Instance.new("Frame")
loadingBarFill.Size = UDim2.new(0, 0, 1, 0) -- Start with 0 width
loadingBarFill.BackgroundColor3 = Color3.fromRGB(255, 206, 9)
loadingBarFill.BorderSizePixel = 0
loadingBarFill.Parent = loadingBarFrame

local loadingBarCorner = Instance.new("UICorner")
loadingBarCorner.CornerRadius = UDim.new(0, 5)
loadingBarCorner.Parent = loadingBarFrame

local loadingBarFillCorner = Instance.new("UICorner")
loadingBarFillCorner.CornerRadius = UDim.new(0, 5)
loadingBarFillCorner.Parent = loadingBarFill

-- Animate loading bar
local TweenService = game:GetService("TweenService")
local loadingTween = TweenService:Create(
    loadingBarFill,
    TweenInfo.new(2, Enum.EasingStyle.Linear), -- 2 seconds duration
    {Size = UDim2.new(1, 0, 1, 0)} -- Full width
)
loadingTween:Play()

-- Destroy loading bar after animation
loadingTween.Completed:Connect(function()
    wait(0.7) -- Brief pause before destroying
    loadingBarFrame:Destroy()
end)

-- Анимация появления nig2aotsos
local targetText = "nig2aotsos"
wait(2)
for i = 1, #targetText do
    mainLabel.Text = string.sub(targetText, 1, i)
    wait(0.1)
end

-- Функция для анимации падения
local function animateFall(label, duration)
    local startTime = tick()
    local initialY = label.Position.Y.Offset
    local velocity = 0
    local gravity = 1000
    
    while tick() - startTime < duration do
        local deltaTime = tick() - startTime
        velocity = velocity + gravity * deltaTime
        local newY = initialY + velocity * deltaTime
        label.Position = UDim2.new(
            label.Position.X.Scale,
            label.Position.X.Offset,
            label.Position.Y.Scale,
            newY
        )
        game:GetService("RunService").RenderStepped:Wait()
    end
end

-- Функция для создания главного фрейма
local function createMainFrame()
    -- Главный фрейм
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 500, 0, 300)
    frame.Position = UDim2.new(0.5, -250, 0.5, -150)
    frame.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
    frame.BackgroundTransparency = 0
    frame.BorderSizePixel = 0
    frame.Parent = screenGui
    
    -- Градиент для фрейма
    local frameGradient = Instance.new("UIGradient")
    frameGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(23, 24, 30)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 206, 9))
    }
    frameGradient.Parent = frame
    
    -- Скругленные углы
    local frameCorner = Instance.new("UICorner")
    frameCorner.CornerRadius = UDim.new(0, 10)
    frameCorner.Parent = frame

    -- Кнопка сворачивания/разворачивания
    local toggleButton = Instance.new("TextButton")
    toggleButton.Size = UDim2.new(0, 60, 0, 60)
    toggleButton.Position = UDim2.new(0.5, -30, 0.5, -30)
    toggleButton.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
    toggleButton.Text = "HUB"
    toggleButton.TextColor3 = Color3.fromRGB(255, 206, 9)
    toggleButton.TextSize = 20
    toggleButton.Font = Enum.Font.Gotham
    toggleButton.Visible = false
    toggleButton.Parent = screenGui
    
    local toggleCorner = Instance.new("UICorner")
    toggleCorner.CornerRadius = UDim.new(0, 8)
    toggleCorner.Parent = toggleButton
    
    -- Панель разделов (справа)
    local sectionPanel = Instance.new("Frame")
    sectionPanel.Size = UDim2.new(0, 100, 0, 300)
    sectionPanel.Position = UDim2.new(1, -100, 0, 0)
    sectionPanel.BackgroundColor3 = Color3.fromRGB(255, 206, 9)
    sectionPanel.BackgroundTransparency = 0.2
    sectionPanel.BorderSizePixel = 0
    sectionPanel.Parent = frame
    
    local sectionCorner = Instance.new("UICorner")
    sectionCorner.CornerRadius = UDim.new(0, 8)
    sectionCorner.Parent = sectionPanel

    -- Контейнер для контента (слева)
    local contentFrame = Instance.new("Frame")
    contentFrame.Size = UDim2.new(0, 400, 0, 300)
    contentFrame.Position = UDim2.new(0, 0, 0, 0)
    contentFrame.BackgroundTransparency = 1
    contentFrame.Parent = frame

    -- Список разделов
    local sections = {"Char", "Discord", "Game", "Visual", "Hub", "Univ"}
    local sectionFrames = {}
    local currentFrame = nil
    local openPanel = nil

    -- Функция для управления панелями
    local function togglePanel(panel, button)
        if openPanel and openPanel ~= panel then
            return
        end
        panel.Visible = not panel.Visible
        openPanel = panel.Visible and panel or nil
    end

    -- Создание кнопок и фреймов для разделов
    for i, section in ipairs(sections) do
        -- Кнопка раздела
        local button = Instance.new("TextButton")
        button.Size = UDim2.new(0, 90, 0, 40)
        button.Position = UDim2.new(0, 5, 0, (i-1) * 45 + 5)
        button.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
        button.Text = section
        button.TextColor3 = Color3.fromRGB(255, 206, 9)
        button.TextSize = 18
        button.Font = Enum.Font.Gotham
        button.Parent = sectionPanel
        
        local buttonCorner = Instance.new("UICorner")
        buttonCorner.CornerRadius = UDim.new(0, 6)
        buttonCorner.Parent = button
        
        -- Эффект при наведении
        button.MouseEnter:Connect(function()
            game:GetService("TweenService"):Create(
                button,
                TweenInfo.new(0.2),
                {BackgroundColor3 = Color3.fromRGB(255, 206, 9)}
            ):Play()
        end)
        button.MouseLeave:Connect(function()
            game:GetService("TweenService"):Create(
                button,
                TweenInfo.new(0.2),
                {BackgroundColor3 = Color3.fromRGB(23, 24, 30)}
            ):Play()
        end)

        -- Фрейм для раздела
        local sectionFrame = Instance.new("Frame")
        sectionFrame.Size = UDim2.new(1, 0, 1, 0)
        sectionFrame.Position = UDim2.new(0, 0, 0, 0)
        sectionFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
        sectionFrame.BackgroundTransparency = 0.5
        sectionFrame.Visible = false
        sectionFrame.Parent = contentFrame
        
        local sectionFrameCorner = Instance.new("UICorner")
        sectionFrameCorner.CornerRadius = UDim.new(0, 8)
        sectionFrameCorner.Parent = sectionFrame

        -- Контент для раздела Char
        if section == "Char" then
            -- Кнопка Walk Speed
            local walkSpeedButton = Instance.new("TextButton")
            walkSpeedButton.Size = UDim2.new(0, 150, 0, 40)
            walkSpeedButton.Position = UDim2.new(0, 20, 0, 20)
            walkSpeedButton.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
            walkSpeedButton.Text = "Walk Speed"
            walkSpeedButton.TextColor3 = Color3.fromRGB(255, 206, 9)
            walkSpeedButton.TextSize = 18
            walkSpeedButton.Font = Enum.Font.Gotham
            walkSpeedButton.ZIndex = 1
