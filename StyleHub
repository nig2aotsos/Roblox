    -- Логика для кнопок
    solaraHubButton.MouseButton1Click:Connect(function()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/samuraa1/Solara-Hub/refs/heads/main/Solara%20Hub.lua"))()
        end)
    end)

    ezHubButton.MouseButton1Click:Connect(function()
        pcall(function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/debug420/Ez-Industries-Launcher-Data/master/Launcher.lua', true))()
        end)
    end)

    magmaHubButton.MouseButton1Click:Connect(function()
        pcall(function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/Commandcracker/Magma-Hub/main/main.lua'))()
        end)
    end)

    airHubButton.MouseButton1Click:Connect(function()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Exunys/AirHub/main/AirHub.lua"))()
        end)
    end)

    trengleHubButton.MouseButton1Click:Connect(function()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Fantemil/Trenglehub/main/trenglehub.lua"))()
        end)
    end)

    externityHubButton.MouseButton1Click:Connect(function()
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/PServerYT/ExternityHub/main/ExternityHub.lua"))()
        end)
    end)

        elseif section == "Univ" then
            -- Кнопка Orca
            local orcaButton = Instance.new("TextButton")
            orcaButton.Size = UDim2.new(0, 150, 0, 40)
            orcaButton.Position = UDim2.new(0, 20, 0, 20)
            orcaButton.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
            orcaButton.Text = "Orca"
            orcaButton.TextColor3 = Color3.fromRGB(127, 71, 227)
            orcaButton.TextSize = 18
            orcaButton.Font = Enum.Font.Gotham
            orcaButton.ZIndex = 1
            orcaButton.Parent = sectionFrame
            
            local orcaButtonCorner = Instance.new("UICorner")
            orcaButtonCorner.CornerRadius = UDim.new(0, 6)
            orcaButtonCorner.Parent = orcaButton

            -- Кнопка Infinite Yield
            local infiniteYieldButton = Instance.new("TextButton")
            infiniteYieldButton.Size = UDim2.new(0, 150, 0, 40)
            infiniteYieldButton.Position = UDim2.new(0, 20, 0, 80)
            infiniteYieldButton.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
            infiniteYieldButton.Text = "Infinite Yield"
            infiniteYieldButton.TextColor3 = Color3.fromRGB(216, 79, 81)
            infiniteYieldButton.TextSize = 18
            infiniteYieldButton.Font = Enum.Font.Gotham
            infiniteYieldButton.ZIndex = 1
            infiniteYieldButton.Parent = sectionFrame
            
            local infiniteYieldButtonCorner = Instance.new("UICorner")
            infiniteYieldButtonCorner.CornerRadius = UDim.new(0, 6)
            infiniteYieldButtonCorner.Parent = infiniteYieldButton

            -- Кнопка Nameless Admin
            local namelessAdminButton = Instance.new("TextButton")
            namelessAdminButton.Size = UDim2.new(0, 150, 0, 40)
            namelessAdminButton.Position = UDim2.new(0, 20, 0, 140)
            namelessAdminButton.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
            namelessAdminButton.Text = "Nam. Admin"
            namelessAdminButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            namelessAdminButton.TextSize = 18
            namelessAdminButton.Font = Enum.Font.Gotham
            namelessAdminButton.ZIndex = 1
            namelessAdminButton.Parent = sectionFrame
            
            local namelessAdminButtonCorner = Instance.new("UICorner")
            namelessAdminButtonCorner.CornerRadius = UDim.new(0, 6)
            namelessAdminButtonCorner.Parent = namelessAdminButton

            -- Кнопка Dex
            local dexButton = Instance.new("TextButton")
            dexButton.Size = UDim2.new(0, 150, 0, 40)
            dexButton.Position = UDim2.new(0, 20, 0, 200)
            dexButton.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
            dexButton.Text = "Dex"
            dexButton.TextColor3 = Color3.fromRGB(19, 162, 213)
            dexButton.TextSize = 18
            dexButton.Font = Enum.Font.Gotham
            dexButton.ZIndex = 1
            dexButton.Parent = sectionFrame
            
            local dexButtonCorner = Instance.new("UICorner")
            dexButtonCorner.CornerRadius = UDim.new(0, 6)
            dexButtonCorner.Parent = dexButton

            orcaButton.MouseButton1Click:Connect(function()
                loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/richie0866/orca/master/public/snapshot.lua"))()
            end)

            infiniteYieldButton.MouseButton1Click:Connect(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
            end)

            namelessAdminButton.MouseButton1Click:Connect(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/ltseverydayyou/Nameless-Admin/main/Source.lua"))()
            end)

            dexButton.MouseButton1Click:Connect(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/infyiff/backup/main/dex.lua"))()
            end)

        else
            local sectionLabel = Instance.new("TextLabel")
            sectionLabel.Size = UDim2.new(0, 380, 0, 280)
            sectionLabel.Position = UDim2.new(0.5, -190, 0.5, -140)
            sectionLabel.Text = "Content for " .. section
            sectionLabel.TextColor3 = Color3.fromRGB(255, 206, 9)
            sectionLabel.TextSize = 24
            sectionLabel.Font = Enum.Font.Gotham
            sectionLabel.BackgroundTransparency = 1
            sectionLabel.TextWrapped = true
            sectionLabel.Parent = sectionFrame
        end

        sectionFrames[section] = sectionFrame

        button.MouseButton1Click:Connect(function()
            if currentFrame then
                currentFrame.Visible = false
            end
            sectionFrame.Visible = true
            currentFrame = sectionFrame
        end)
    end

    if sectionFrames[sections[1]] then
        sectionFrames[sections[1]].Visible = true
        currentFrame = sectionFrames[sections[1]]
    end

    -- Кнопка сворачивания
    local minimizeButton = Instance.new("TextButton")
    minimizeButton.Size = UDim2.new(0, 25, 0, 25)
    minimizeButton.Position = UDim2.new(0, 470, 0, 272.5)
    minimizeButton.BackgroundColor3 = Color3.fromRGB(255, 206, 9)
    minimizeButton.Text = "–"
    minimizeButton.TextColor3 = Color3.fromRGB(23, 24, 30)
    minimizeButton.TextSize = 20
    minimizeButton.Font = Enum.Font.Gotham
    minimizeButton.Parent = frame
    
    local minimizeButtonCorner = Instance.new("UICorner")
    minimizeButtonCorner.CornerRadius = UDim.new(0, 6)
    minimizeButtonCorner.Parent = minimizeButton
    
    -- Эффект при наведении
    minimizeButton.MouseEnter:Connect(function()
        game:GetService("TweenService"):Create(
            minimizeButton,
            TweenInfo.new(0.2),
            {BackgroundColor3 = Color3.fromRGB(23, 24, 30)}
        ):Play()
    end)
    minimizeButton.MouseLeave:Connect(function()
        game:GetService("TweenService"):Create(
            minimizeButton,
            TweenInfo.new(0.2),
            {BackgroundColor3 = Color3.fromRGB(255, 206, 9)}
        ):Play()
    end)

    minimizeButton.MouseButton1Click:Connect(function()
        frame.Visible = false
        toggleButton.Visible = true
    end)

    toggleButton.MouseButton1Click:Connect(function()
        frame.Visible = true
        toggleButton.Visible = false
    end)

    -- Кнопка закрытия
    local closeButton = Instance.new("TextButton")
    closeButton.Size = UDim2.new(0, 25, 0, 25)
    closeButton.Position = UDim2.new(0, 405, 0, 272.5)
    closeButton.BackgroundColor3 = Color3.fromRGB(255, 206, 9)
    closeButton.Text = "✖"
    closeButton.TextColor3 = Color3.fromRGB(23, 24, 30)
    closeButton.TextSize = 18
    closeButton.Font = Enum.Font.Gotham
    closeButton.Parent = frame
    
    local closeButtonCorner = Instance.new("UICorner")
    closeButtonCorner.CornerRadius = UDim.new(0, 6)
    closeButtonCorner.Parent = closeButton
    
    -- Эффект при наведении
    closeButton.MouseEnter:Connect(function()
        game:GetService("TweenService"):Create(
            closeButton,
            TweenInfo.new(0.2),
            {BackgroundColor3 = Color3.fromRGB(23, 24, 30)}
        ):Play()
    end)
    closeButton.MouseLeave:Connect(function()
        game:GetService("TweenService"):Create(
            closeButton,
            TweenInfo.new(0.2),
            {BackgroundColor3 = Color3.fromRGB(255, 206, 9)}
        ):Play()
    end)

    -- Диалоговое окно
    local function createConfirmDialog()
        frame.Visible = false
        toggleButton.Visible = false

        local dialogFrame = Instance.new("Frame")
        dialogFrame.Size = UDim2.new(0, 250, 0, 150)
        dialogFrame.Position = UDim2.new(0.5, -125, 0.5, -75)
        dialogFrame.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
        dialogFrame.BackgroundTransparency = 0
        dialogFrame.BorderSizePixel = 0
        dialogFrame.Parent = screenGui
        
        local dialogFrameCorner = Instance.new("UICorner")
        dialogFrameCorner.CornerRadius = UDim.new(0, 8)
        dialogFrameCorner.Parent = dialogFrame

        local dialogText = Instance.new("TextLabel")
        dialogText.Size = UDim2.new(0, 230, 0, 50)
        dialogText.Position = UDim2.new(0.5, -115, 0, 20)
        dialogText.Text = "Вы точно хотите закрыть скрипт?"
        dialogText.TextColor3 = Color3.fromRGB(255, 206, 9)
        dialogText.TextSize = 16
        dialogText.Font = Enum.Font.Gotham
        dialogText.BackgroundTransparency = 1
        dialogText.TextWrapped = true
        dialogText.Parent = dialogFrame

        local yesButton = Instance.new("TextButton")
        yesButton.Size = UDim2.new(0, 80, 0, 40)
        yesButton.Position = UDim2.new(0, 30, 0, 90)
        yesButton.BackgroundColor3 = Color3.fromRGB(255, 206, 9)
        yesButton.Text = "Нет"
        yesButton.TextColor3 = Color3.fromRGB(23, 24, 30)
        yesButton.TextSize = 18
        yesButton.Font = Enum.Font.Gotham
        yesButton.Parent = dialogFrame
        
        local yesButtonCorner = Instance.new("UICorner")
        yesButtonCorner.CornerRadius = UDim.new(0, 6)
        yesButtonCorner.Parent = yesButton

        local noButton = Instance.new("TextButton")
        noButton.Size = UDim2.new(0, 80, 0, 40)
        noButton.Position = UDim2.new(0, 140, 0, 90)
        noButton.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
        noButton.Text = "Да"
        noButton.TextColor3 = Color3.fromRGB(255, 206, 9)
        noButton.TextSize = 18
        noButton.Font = Enum.Font.Gotham
        noButton.Parent = dialogFrame
        
        local noButtonCorner = Instance.new("UICorner")
        noButtonCorner.CornerRadius = UDim.new(0, 6)
        noButtonCorner.Parent = noButton

        yesButton.MouseButton1Click:Connect(function()
            dialogFrame:Destroy()
            frame.Visible = true
        end)

        noButton.MouseButton1Click:Connect(function()
            screenGui:Destroy()
        end)
    end

    closeButton.MouseButton1Click:Connect(createConfirmDialog)

    -- Перетаскивание фрейма
    local dragging
    local dragInput
    local dragStart
    local startPos

    frame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = frame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)

    frame.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            dragInput = input
        end
    end)

    game:GetService("UserInputService").InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - dragStart
            frame.Position = UDim2.new(
                startPos.X.Scale,
                startPos.X.Offset + delta.X,
                startPos.Y.Scale,
                startPos.Y.Offset + delta.Y
            )
            toggleButton.Position = UDim2.new(
                frame.Position.X.Scale,
                frame.Position.X.Offset + frame.Size.X.Offset / 2 - toggleButton.Size.X.Offset / 2,
                frame.Position.Y.Scale,
                frame.Position.Y.Offset + frame.Size.Y.Offset / 2 - toggleButton.Size.Y.Offset / 2
            )
        end
    end)

    -- Перетаскивание кнопки S/H
    local toggleDragging
    local toggleDragInput
    local toggleDragStart
    local toggleStartPos

    toggleButton.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            toggleDragging = true
            toggleDragStart = input.Position
            toggleStartPos = toggleButton.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    toggleDragging = false
                end
            end)
        end
    end)

    toggleButton.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            toggleDragInput = input
        end
    end)

    game:GetService("UserInputService").InputChanged:Connect(function(input)
        if input == toggleDragInput and toggleDragging then
            local delta = input.Position - toggleDragStart
            toggleButton.Position = UDim2.new(
                toggleStartPos.X.Scale,
                toggleStartPos.X.Offset + delta.X,
                toggleStartPos.Y.Scale,
                toggleStartPos.Y.Offset + delta.Y
            )
        end
    end)
end

-- Запуск анимации и создание фрейма
wait(0.5)
spawn(function()
    animateFall(byLabel, 1)
    byLabel:Destroy()
end)
spawn(function()
    animateFall(mainLabel, 1)
    mainLabel:Destroy()
end)
wait(1)
createMainFrame() 
