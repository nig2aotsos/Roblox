            -- Логика ESP
            local espEnabled = false
            local espConnections = {}
            local espHighlights = {}

            local function setESP(enabled)
                espEnabled = enabled
                espToggleButton.Text = enabled and "On" or "Off"
                espToggleButton.BackgroundColor3 = enabled and Color3.fromRGB(80, 120, 80) or Color3.fromRGB(120, 60, 60)

                for _, highlight in pairs(espHighlights) do
                    highlight:Destroy()
                end
                espHighlights = {}

                for _, connection in pairs(espConnections) do
                    connection:Disconnect()
                end
                espConnections = {}

                if enabled then
                    local function addESP(player)
                        if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            local highlight = Instance.new("Highlight")
                            highlight.FillColor = player.TeamColor.Color
                            highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
                            highlight.FillTransparency = 0.5
                            highlight.OutlineTransparency = 0
                            highlight.Adornee = player.Character
                            highlight.Parent = player.Character
                            espHighlights[player] = highlight

                            local teamConnection
                            teamConnection = player:GetPropertyChangedSignal("Team"):Connect(function()
                                if espHighlights[player] then
                                    espHighlights[player].FillColor = player.TeamColor.Color
                                end
                            end)
                            table.insert(espConnections, teamConnection)
                        end
                    end

                    for _, player in ipairs(game.Players:GetPlayers()) do
                        addESP(player)
                    end

                    table.insert(espConnections, game.Players.PlayerAdded:Connect(function(player)
                        player.CharacterAdded:Connect(function()
                            addESP(player)
                        end)
                    end))

                    for _, player in ipairs(game.Players:GetPlayers()) do
                        if player ~= game.Players.LocalPlayer then
                            table.insert(espConnections, player.CharacterAdded:Connect(function()
                                addESP(player)
                            end))
                        end
                    end
                end
            end

            game.Players.LocalPlayer.CharacterAdded:Connect(function()
                if espEnabled then
                    setESP(true)
                end
            end)

            espToggleButton.MouseButton1Click:Connect(function()
                setESP(not espEnabled)
            end)

            local espBind = Enum.KeyCode.Z
            espBindInput.FocusLost:Connect(function()
                local key = espBindInput.Text:upper()
                if Enum.KeyCode[key] then
                    espBind = Enum.KeyCode[key]
                else
                    espBindInput.Text = espBind.Name
                end
            end)

            local UserInputService = game:GetService("UserInputService")
            UserInputService.InputBegan:Connect(function(input, gameProcessed)
                if not gameProcessed and input.KeyCode == espBind then
                    setESP(not espEnabled)
                end
            end)

            espButton.MouseButton1Click:Connect(function()
                togglePanel(espPanel, espButton)
            end)

