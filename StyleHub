            speedPanel.Visible = false
            speedPanel.ZIndex = 2
            speedPanel.Parent = sectionFrame
            
            local speedPanelCorner = Instance.new("UICorner")
            speedPanelCorner.CornerRadius = UDim.new(0, 8)
            speedPanelCorner.Parent = speedPanel

            for _, child in ipairs(speedPanel:GetDescendants()) do
                if child:IsA("GuiObject") then
                    child.ZIndex = 2
                end
            end

            local speedInput = Instance.new("TextBox")
            speedInput.Size = UDim2.new(0, 100, 0, 30)
            speedInput.Position = UDim2.new(0, 10, 0, 10)
            speedInput.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
