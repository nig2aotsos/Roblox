            animScriptsPanelCorner.Parent = animScriptsPanel

            for _, child in ipairs(animScriptsPanel:GetDescendants()) do
                if child:IsA("GuiObject") then
                    child.ZIndex = 2
                end
            end

            local aquaMatrixButton = Instance.new("TextButton")
            aquaMatrixButton.Size = UDim2.new(0, 150, 0, 40)
            aquaMatrixButton.Position = UDim2.new(0, 10, 0, 10)
            aquaMatrixButton.BackgroundColor3 = Color3.fromRGB(23, 24, 30)
            aquaMatrixButton.Text = "AquaMatrix"
            aquaMatrixButton.TextColor3 = Color3.fromRGB(255, 206, 9)
            aquaMatrixButton.TextSize = 18
            aquaMatrixButton.Font = Enum.Font.Gotham
            aquaMatrixButton.ZIndex = 2
            aquaMatrixButton.Parent = animScriptsPanel
            
            local aquaMatrixButtonCorner = Instance.new("UICorner")
            aquaMatrixButtonCorner.CornerRadius = UDim.new(0, 6)
            aquaMatrixButtonCorner.Parent = aquaMatrixButton

            -- Логика для кнопок
            walkSpeedButton.MouseButton1Click:Connect(function()
                togglePanel(speedPanel, walkSpeedButton)
            end)

            noClipButton.MouseButton1Click:Connect(function()
                togglePanel(noClipPanel, noClipButton)
            end)

            teleportButton.MouseButton1Click:Connect(function()
                togglePanel(teleportPanel, teleportButton)
            end)

            flyButton.MouseButton1Click:Connect(function()
                togglePanel(flyPanel, flyButton)
            end)

            animScriptsButton.MouseButton1Click:Connect(function()
                togglePanel(animScriptsPanel, animScriptsButton)
            end)

            aquaMatrixButton.MouseButton1Click:Connect(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/ExploitFin/AquaMatrix/refs/heads/AquaMatrix/AquaMatrix"))()
            end)

            -- Логика Walk Speed
            local function applySpeed()
                local player = game.Players.LocalPlayer
                local speed = tonumber(speedInput.Text)
                if player.Character and player.Character:FindFirstChild("Humanoid") and speed then
                    player.Character.Humanoid.WalkSpeed = speed
                end
            end

            offButton.MouseButton1Click:Connect(function()
                local player = game.Players.LocalPlayer
                if player.Character and player.Character:FindFirstChild("Humanoid") then
                    player.Character.Humanoid.WalkSpeed = 16
                    speedInput.Text = "16"
                end
            end)

            local UserInputService = game:GetService("UserInputService")
            local speedBind = Enum.KeyCode.E
            bindInput.FocusLost:Connect(function()
                local key = bindInput.Text:upper()
                if Enum.KeyCode[key] then
                    speedBind = Enum.KeyCode[key]
                else
                    bindInput.Text = speedBind.Name
                end
            end)

            UserInputService.InputBegan:Connect(function(input, gameProcessed)
                if not gameProcessed and input.KeyCode == speedBind then
                    applySpeed()
                end
            end)

            -- Логика NoClip
            local noClipEnabled = false
            local noClipConnection

            local function setNoClip(enabled)
                local player = game.Players.LocalPlayer
                noClipEnabled = enabled
                noClipToggleButton.Text = enabled and "On" or "Off"
                noClipToggleButton.BackgroundColor3 = enabled and Color3.fromRGB(80, 120, 80) or Color3.fromRGB(120, 60, 60)

                if noClipConnection then
                    noClipConnection:Disconnect()
                    noClipConnection = nil
                end

                if enabled then
                    noClipConnection = game:GetService("RunService").Stepped:Connect(function()
                        if player.Character then
                            for _, part in ipairs(player.Character:GetDescendants()) do
                                if part:IsA("BasePart") then
                                    part.CanCollide = false
                                end
                            end
                        end
                    end)
                else
                    if player.Character then
                        for _, part in ipairs(player.Character:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.CanCollide = true
                            end
                        end
                    end
                end
            end

            game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
                if noClipEnabled then
                    setNoClip(true)
                end
            end)

            noClipToggleButton.MouseButton1Click:Connect(function()
                setNoClip(not noClipEnabled)
            end)

            local noClipBind = Enum.KeyCode.Q
            noClipBindInput.FocusLost:Connect(function()
                local key = noClipBindInput.Text:upper()
                if Enum.KeyCode[key] then
                    noClipBind = Enum.KeyCode[key]
                else
                    noClipBindInput.Text = noClipBind.Name
                end
            end)

            UserInputService.InputBegan:Connect(function(input, gameProcessed)
                if not gameProcessed and input.KeyCode == noClipBind then
                    setNoClip(not noClipEnabled)
                end
            end)

            -- Логика телепортации
            local function teleportToPlayer(playerName)
                local localPlayer = game.Players.LocalPlayer
                if not localPlayer.Character or not localPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    return
                end

                local targetPlayer
                for _, player in ipairs(game.Players:GetPlayers()) do
                    if player.Name:lower():find(playerName:lower()) then
                        targetPlayer = player
                        break
                    end
                end

                if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    localPlayer.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)
                end
            end

            tpButton.MouseButton1Click:Connect(function()
                local playerName = playerNameInput.Text
                if playerName ~= "" then
                    teleportToPlayer(playerName)
                end
            end)

            -- Логика Fly
            local flyEnabled = false
            local flyConnection
            local flySpeed = 50

            local function setFly(enabled)
                local player = game.Players.LocalPlayer
                flyEnabled = enabled
                flyToggleButton.Text = enabled and "On" or "Off"
                flyToggleButton.BackgroundColor3 = enabled and Color3.fromRGB(80, 120, 80) or Color3.fromRGB(120, 60, 60)

                if flyConnection then
                    flyConnection:Disconnect()
                    flyConnection = nil
                end

                if enabled then
                    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        local humanoid = player.Character.Humanoid
                        local rootPart = player.Character.HumanoidRootPart
                        humanoid.PlatformStand = true

                        local bodyVelocity = Instance.new("BodyVelocity")
                        bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
                        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
                        bodyVelocity.Parent = rootPart
                        bodyVelocity.Name = "FlyVelocity"

                        local function updateFlySpeed()
                            local newSpeed = tonumber(flySpeedInput.Text)
                            if newSpeed then
                                flySpeed = newSpeed
                            else
                                flySpeedInput.Text = tostring(flySpeed)
                            end
                        end

                        updateFlySpeed()
                        flyConnection = game:GetService("RunService").RenderStepped:Connect(function()
                            if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
                                setFly(false)
                                return
                            end

                            local camera = game.Workspace.CurrentCamera
                            local moveDirection = Vector3.new(0, 0, 0)
                            local input = UserInputService:GetKeysPressed()

                            for _, key in ipairs(input) do
                                if key.KeyCode == Enum.KeyCode.W then
                                    moveDirection = moveDirection + camera.CFrame.LookVector
                                end
                                if key.KeyCode == Enum.KeyCode.S then
                                    moveDirection = moveDirection - camera.CFrame.LookVector
                                end
                                if key.KeyCode == Enum.KeyCode.A then
                                    moveDirection = moveDirection - camera.CFrame.RightVector
                                end
                                if key.KeyCode == Enum.KeyCode.D then
                                    moveDirection = moveDirection + camera.CFrame.RightVector
                                end
                                if key.KeyCode == Enum.KeyCode.Space then
                                    moveDirection = moveDirection + Vector3.new(0, 1, 0)
                                end
                                if key.KeyCode == Enum.KeyCode.LeftShift then
                                    moveDirection = moveDirection - Vector3.new(0, 1, 0)
                                end
                            end

                            if moveDirection.Magnitude > 0 then
                                moveDirection = moveDirection.Unit * flySpeed
                            end
                            bodyVelocity.Velocity = moveDirection
                        end)

                        flySpeedInput.FocusLost:Connect(updateFlySpeed)
                    end
                else
                    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        local humanoid = player.Character.Humanoid
                        local rootPart = player.Character.HumanoidRootPart
                        humanoid.PlatformStand = false
                        local bodyVelocity = rootPart:FindFirstChild("FlyVelocity")
                        if bodyVelocity then
                            bodyVelocity:Destroy()
                        end
                    end
                end
            end

            game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
                if flyEnabled then
                    setFly(true)
                end
            end)

            flyToggleButton.MouseButton1Click:Connect(function()
                setFly(not flyEnabled)
            end)

            local flyBind = Enum.KeyCode.F
            flyBindInput.FocusLost:Connect(function()
                local key = flyBindInput.Text:upper()
                if Enum.KeyCode[key] then
                    flyBind = Enum.KeyCode[key]
                else
                    flyBindInput.Text = flyBind.Name
                end
            end)

            UserInputService.InputBegan:Connect(function(input, gameProcessed)
                if not gameProcessed and input.KeyCode == flyBind then
                    setFly(not flyEnabled)
                end
            end)

