    -- Эффект при наведении
    ezHubButton.MouseEnter:Connect(function()
        game:GetService("TweenService"):Create(
            ezHubButton,
            TweenInfo.new(0.2),
            {BackgroundColor3 = Color3.fromRGB(255, 206, 9)}
        ):Play()
    end)
    ezHubButton.MouseLeave:Connect(function()
        game:GetService("TweenService"):Create(
            ezHubButton,
            TweenInfo.new(0.2),
            {BackgroundColor3 = Color3.fromRGB(23, 24, 30)}
        ):Play()
    end)

    -- Кнопка Magma Hub
